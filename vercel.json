{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "engines": {
        "node": "20.x"
      }
}
